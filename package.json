{
  "name": "shortening",
  "version": "1.0.0",
  "description": "shortening service returns short url from an original url and the original url from the same short url",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "server:prod": "node dist/server.js",
    "server:dev": "ts-node-dev --exit-child --respawn -r tsconfig-paths/register src/server.ts",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "prettier": "prettier --check \"{*.js,!(node*)**/*.ts}\"",
    "prettier:fix": "prettier --write \"{*.js,!(node*)**/*.ts}\"",
    "loglevel:change": "runtime-node-refresh",
    "test": "jest -c jest.config.js --env=node",
    "test:cov": "jest -c jest.config.js --coverage --env=node",
    "test:watch": "jest -c jest.config.js -i --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "http://finn-gmbh-qminbi@git.codesubmit.io/finn-gmbh/the-shortest-url-jhdtei"
  },
  "keywords": [
    "shortening app",
    "url shortener app"
  ],
  "author": "AhmedAzzo",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/supertest": "^2.0.12",
    "apicache": "^1.6.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-http-context": "^1.2.4",
    "fs": "^0.0.1-security",
    "http-status": "^1.6.2",
    "joi": "^17.9.2",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "node-mocks-http": "^1.12.2",
    "path": "^0.12.7",
    "runtime-node-refresh": "^1.0.7",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^3.12.0"
  }
}
